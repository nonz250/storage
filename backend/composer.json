        "fig/http-message-util": "^1.1",
        "laminas/laminas-diactoros": "^2.8",
        "laminas/laminas-httphandlerrunner": "^2.1",
        "league/container": "^4.2",
        "league/route": "^5.1",
        "vlucas/phpdotenv": "^5.4",
        "symfony/console": "^5.4",
        "symfony/uid": "^5.4",
        "monolog/monolog": "^2.5"
