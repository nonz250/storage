{
    "name": "nonz250/storage",
    "description": "My Storage",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nonz250\\Storage\\App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
